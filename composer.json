{
    "name": "blackfire/player",
    "type": "project",
    "description": "A powerful web crawler and web scraper with Blackfire support",
    "keywords": ["scraper", "crawler", "blackfire"],
    "homepage": "https://blackfire.io/player",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@blackfire.io",
            "homepage": "https://blackfire.io/",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "blackfire/php-sdk": "^1.6.0",
        "fzaninotto/faker": "^1.5",
        "guzzlehttp/guzzle": "^6.1",
        "mtdowling/jmespath.php": "^2.2",
        "psr/log": "^1.0",
        "symfony/console": "^3.2",
        "symfony/css-selector": "^3.2",
        "symfony/dom-crawler": "^3.2",
        "symfony/expression-language": "^3.2",
        "symfony/filesystem": "^3.2",
        "symfony/var-dumper": "^3.2",
        "symfony/yaml": "^3.2",
        "webmozart/glob": "^4.0"
    },
    "require-dev": {
        "symfony/finder": "^3.2",
        "symfony/process": "^3.2",
        "symfony/phpunit-bridge": "^3.2"
    },
    "autoload": {
        "psr-4" : {
            "Blackfire\\Player\\" : "Player"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "platform": {
            "php": "5.5.9"
        }
    },
    "bin": ["bin/blackfire-player"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
